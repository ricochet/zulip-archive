    {
        "content": "<p>alexcrichton merged <a href=\"https://github.com/bytecodealliance/wasmtime/pull/9910\">PR #9910</a>.</p>",
        "id": 491309022,
        "sender_full_name": "Wasmtime GitHub notifications bot",
